{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "${project_url}"
  },
  "license": "${license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.base.MultiModBase"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader}",
    "fabric": "*",
    "minecraft": ">=${minecraft} <${max_minecraft}${extra}"
  }
}